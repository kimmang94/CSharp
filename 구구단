using System;

namespace CSharp_basic
{
    class Program
    {
       
        static void Main(string[] args)
        {
            // 구구단 예제

            for (int i = 1; i < 10; i++)
            {
                Console.WriteLine($"{i}단");
                for (int j = 1; j < 10; j++)
                {
                    Console.WriteLine($"{i} x {j} = {i * j}");
                }
                
            }
        }

    }
}
