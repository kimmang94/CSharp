using System;

namespace CSharp_basic
{
    class Program
    {
        
        static void Main(string[] args)
        {
            // 배열
            int[] scores = new int[5];

            // 0 1 2 3 4 
            scores[0] = 10;
            scores[1] = 20;
            scores[2] = 30;
            scores[3] = 40;
            scores[4] = 50;
                

            for (int i = 0; i < 5; i++)
            {
                Console.WriteLine(scores[i]);
            }
        }

    }
}
