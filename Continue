using System;

namespace CSharp_basic
{
    class Program
    {
        static void Main(string[] args)
        {
            // Continue 
            for (int i = 0; i <= 100; i++)
            {
                if ((i % 3) != 0)
                    continue;

                Console.WriteLine($"3으로 나뉘는 숫자 발견 : {i}");
            }
        }

    }
}
