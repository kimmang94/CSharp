using System;

namespace ConsoleApp1
{
    class HelloWorld
    {
        static void Main(string[] args)
        {
            // [1] 데이터타입 변수명 = 리터럴값;
            int hp = 100;
            int mp = 50;
            
            Console.WriteLine($"HP : {hp}, MP : {mp}");
            // HP : 100, MP : 50
        }
    }
}
